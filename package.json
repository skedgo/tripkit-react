    "scripts": {
        "start": "react-app-rewired start",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject",
        "build:lib": "npm run build:babel:esm && npm run build:types && node ./scripts/copyFiles.mjs",
        "build:babel:esm": "BABEL_ENV=esm babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist && npm run build:babel:svg",
        "build:types": "tsc -p tsconfig-babel.json",
        "build:babel:svg": "babel --extensions \".svg\" --out-file-extension .svg.js src --out-dir dist",
        "clean": "rimraf dist",
        "develop": "npm run build:types && npm run build:babel:esm --skip-initial-build --watch --verbose",
        "start:doc": "styleguidist server",
        "build:doc": "cd docs && mkdocs build && cd .. && styleguidist build",
        "build:mkdocs": "cd docs && mkdocs build"
    },
