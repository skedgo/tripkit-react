  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build:lib": "npm run build:babel && npm run build:types && node ./scripts/copyFiles.js",
    "build:babel": "npm run build:babel:cjs",
    "_build:babel": "concurrently \"npm run build:babel:esm && npm run build:babel:umd\" \"npm run build:babel:cjs\"",
    "build:babel:cjs": "BABEL_ENV=cjs babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/js --presets=@babel/env && npm run build:svg:cjs",
    "build:babel:esm": "BABEL_ENV=esm babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/esm && npm run build:svg:esm",
    "build:babel:umd": "BABEL_ENV=umd babel --source-maps --extensions \".js\" dist/esm --out-dir dist/umd --plugins=transform-es2015-modules-umd && npm run build:svg:umd",
    "build:types": "tsc -p tsconfig.gen-dts.json",
    "build:svg:cjs": "babel --extensions \".svg\" --out-file-extension .svg.js src --out-dir dist/js",
    "build:svg:esm": "babel --extensions \".svg\" --out-file-extension .svg.js src --out-dir dist/esm",
    "build:svg:umd": "babel --extensions \".svg\" --out-file-extension .svg.js src --out-dir dist/umd",
    "clean": "rimraf dist",
    "develop": "npm run build:types && npm run build:babel:esm --skip-initial-build --watch --verbose",
    "start:doc": "styleguidist server",
    "build:doc": "cd docs && mkdocs build && cd .. && styleguidist build",
    "build:mkdocs": "cd docs && mkdocs build"
  },
  "devDependencies": {
